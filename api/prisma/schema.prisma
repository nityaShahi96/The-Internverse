
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
    id      Int @id @default(autoincrement())
    name    String
    email   String @unique
    password    String
    token   Token[]
}

model Token {
    id      Int @id @default(autoincrement())
    token   String @unique
    user    User @relation(fields: [userId], references: [id])
    userId  Int
}


